[run]
source = modules, case-opening-app
omit = 
    */tests/*
    */venv/*
    */__pycache__/*
    */fixtures/*
    setup.py
    conftest.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = Case Opening Sheet Manager - Coverage Report

[xml]
output = coverage.xml